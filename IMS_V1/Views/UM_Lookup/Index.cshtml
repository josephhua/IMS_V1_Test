@model IEnumerable<IMS_V1.UM_Lookup>

@{
    ViewBag.Title = "UM Codes";
}

<br />
<br />
<table>
    <tr>
        <td colspan="3" style="text-align:center;">
            <h2>UM Codes</h2>
        </td>
    </tr>

    <tr>
        <td colspan="3" style="text-align:left;">
            @Html.ActionLink("Create New UM Code", "Create")
        </td>
    </tr>
    <tr>
        <th>
            UM Code
        </th>
        <th>
            UM Description
        </th>
        <th>
            Enabled
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align:center;">
                @Html.DisplayFor(modelItem => item.UM_Code)
            </td>
            <td style="text-align:left;">
                @Html.DisplayFor(modelItem => item.UM_Description)
            </td>
            <td style="text-align:left;">
                @Html.DisplayFor(modelItem => item.Enabled)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.UM_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.UM_Id })
            </td>
        </tr>
    }

</table>
